<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.calculator.MainActivity$PlaceholderFragment"
    android:background="@drawable/activiy_background">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/button1"
        android:layout_marginTop="111dp"
        android:layout_marginLeft="55dp"
        android:longClickable="false"
        android:clickable="true"
        android:minWidth="60dp"
        android:minHeight="60dp"
        android:layout_below="@+id/webView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:id="@+id/button2"
        android:layout_alignTop="@+id/button1"
        android:layout_toRightOf="@+id/button1"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:id="@+id/button3"
        android:layout_alignTop="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:id="@+id/button4"
        android:layout_below="@+id/button1"
        android:layout_alignLeft="@+id/button1"
        android:layout_alignStart="@+id/button1"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:id="@+id/button5"
        android:layout_below="@+id/button1"
        android:layout_toRightOf="@+id/button1"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:id="@+id/button6"
        android:layout_below="@+id/button2"
        android:layout_alignLeft="@+id/button3"
        android:layout_alignStart="@+id/button3"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:id="@+id/button7"
        android:layout_below="@+id/button4"
        android:layout_alignLeft="@+id/button4"
        android:layout_alignStart="@+id/button4"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:id="@+id/button8"
        android:layout_below="@+id/button4"
        android:layout_toRightOf="@+id/button4"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        android:id="@+id/button9"
        android:layout_below="@+id/button6"
        android:layout_toRightOf="@+id/button8"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="."
        android:id="@+id/Pika"
        android:layout_below="@+id/button7"
        android:layout_toRightOf="@+id/button7"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/button0"
        android:layout_below="@+id/button7"
        android:layout_toLeftOf="@+id/Pika"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+"
        android:id="@+id/Plus"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp"
        android:layout_below="@+id/ZbrisiVse"
        android:layout_toRightOf="@+id/button3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C"
        android:id="@+id/ZbrisiVse"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp"
        android:layout_alignTop="@+id/ZbrisiZnak"
        android:layout_toRightOf="@+id/button3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/Minus"
        android:layout_alignTop="@+id/button9"
        android:layout_toRightOf="@+id/button9"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="="
        android:id="@+id/JeEnako"
        android:layout_below="@+id/button8"
        android:layout_toRightOf="@+id/button8"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        android:id="@+id/Deljeno"
        android:layout_below="@+id/Minus"
        android:layout_alignLeft="@+id/Minus"
        android:layout_alignStart="@+id/Minus"
        android:longClickable="false"
        android:minWidth="60dp"
        android:minHeight="60dp" />



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="("
        android:id="@+id/LeviZaklepaj"
        android:minWidth="60dp"
        android:minHeight="60dp"
        android:layout_above="@+id/button2"
        android:layout_alignLeft="@+id/button1"
        android:layout_alignStart="@+id/button1" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=")"
        android:id="@+id/DesniZaklepaj"
        android:minWidth="60dp"
        android:minHeight="60dp"
        android:layout_above="@+id/button2"
        android:layout_toRightOf="@+id/button1" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="B"
        android:id="@+id/ZbrisiZnak"
        android:minWidth="60dp"
        android:minHeight="60dp"
        android:layout_alignTop="@+id/DesniZaklepaj"
        android:layout_toRightOf="@+id/DesniZaklepaj" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*"
        android:id="@+id/Krat"
        android:minWidth="60dp"
        android:minHeight="60dp"
        android:layout_below="@+id/button3"
        android:layout_toRightOf="@+id/button6" />

    <WebView
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:id="@+id/webView"
        android:visibility="visible"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="false" />

</RelativeLayout>
